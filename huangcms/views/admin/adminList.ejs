<% include header %>
	<div class="header">
		<!-- 页面标题 -->
		<h1 class="page-header">管理员列表</h1>
		<!-- 路径导航 -->
		<ol class="breadcrumb">
			<li>
				<a href="#">系统管理</a>
			</li>
			<li class="active">管理员列表</li>
		</ol> 
		<!-- 路径导航 end -->
	</div>
	<div id="page-inner">
		<div class="dashboard-cards">
			<div class="row">
				<table class="table table-hover ">
					<tr class="danger">
						<th>序号</th>
						<th>用户名</th>
						<th>用户介绍</th>
						<th>手机号</th>
						<th>创建时间</th>
						<th>操作</th>
					</tr>
					<% admins.forEach(function(val,i){ %>
					<tr>
						<td><%=i+1%></td>
						<td><%=val.name%></td>
						<td><%=val.info%></td>
						<td><%=val.tel%></td>
						<td><%= val.ctime.toLocaleString() %></td>
						<td>
							<a href="/admin/adminDel/<%=val._id%>" onclick="return confirm('您确定要删除吗?')">
									删除
							</a>
						</td>
					</tr>
					<% })%>
				</table>
			</div>
		</div>
	</div>
<% include footer %>